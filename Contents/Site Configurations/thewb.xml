<?xml version="1.0" encoding="UTF-8"?>
<site site="www.thewb.com"
	    plugin="http://www.thewb.com/player/wbphasethree/wbvideoplayer.swf"
			initialState="playing"
			version="1.0">
	
	<crop x="0" y="0" width="640" height="380" />
	
	<!-- PLAYING -->
	<state name="playing">
    		
		<!-- Video ends -->
		<event>
			<condition>
				<url matches="player/embed/AS3/endslate" />
			</condition>
			<action>
				<goto state="end" />
			</action>
		</event>
	</state>
</site>